entre-nous : le carrosse a été conçu spécialement pour les enfants des Dieux grecs, afin qu'ils soient à l'heure à l'école et ne se fassent pas gronder par Platon en personne.","language":"fr"}],"active":true,"area":10,"type":1,"resortType":0,"open":0,"ageMin":4,"ageMax":0,"sizeMin":100,"sizeMax":0,"thrill":4,"serviceTypes":null,"gastronomyTypes":null,"kitchenType":0,"gastronomyOptions":null,"disabled":false,"pregnant":false,"parentAllowed":true,"media":[{"type":0,"previewImage":[{"value":"https://api.europapark.de/detail-5.7/pegasus_1.jpg","language":"de"},{"value":"https://api.europapark.de/detail-5.7/pegasus_1.jpg","language":"en"},{"value":"https://api.europapark.de/detail-5.7/pegasus_1.jpg","language":"fr"}],"action":null},{"type":0,"previewImage":[{"value":"https://api.europapark.de/detail-5.7/pegasus_2.jpg","language":"de"},{"value":"https://api.europapark.de/detail-5.7/pegasus_2.jpg","language":"en"},{"value":"https://api.europapark.de/detail-5.7/pegasus_2.jpg","language":"fr"}],"action":null},{"type":0,"previewImage":[{"value":"https://api.europapark.de/detail-5.7/pegasus_3.jpg","language":"de"},{"value":"https://api.europapark.de/detail-5.7/pegasus_3.jpg","language":"en"},{"value":"https://api.europapark.de/detail-5.7/pegasus_3.jpg","language":"fr"}],"action":null},{"type":0,"previewImage":[{"value":"https://api.europapark.de/detail-5.7/pegasus_4.jpg","language":"de"},{"value":"https://api.europapark.de/detail-5.7/pegasus_4.jpg","language":"en"},{"value":"https://api.europapark.de/detail-5.7/pegasus_4.jpg","language":"fr"}],"action":null},{"type":0,"previewImage":[{"value":"https://api.europapark.de/detail-5.7/pegasus_5.jpg","language":"de"},{"value":"https://api.europapark.de/detail-5.7/pegasus_5.jpg","language":"en"},{"value":"https://api.europapark.de/detail-5.7/pegasus_5.jpg","language":"fr"}],"action":null}],"attractionsCategories":[1,13],"facebook":"","benefitsOptions":null,"linkedPois":null,"actions":null,"since":2006,"capacity":18,"capacityType":2,