{
    "version": "0.2.0",
    "configurations": [{
        "name": "Online Test",
        "type": "node",
        "request": "launch",
        "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
        "stopOnEntry": false,
        "args": [
            "lib/testonline.js"
        ],
        "cwd": "${workspaceRoot}",
        "preLaunchTask": "",
        "runtimeExecutable": null,
        "runtimeArgs": [
            "--nolazy"
        ],
        "env": {
            "NODE_ENV": "development",
            "NODE_DEBUG": "themeparks",
            "PARKID": "DisneylandResortMagicKingdom"
        },
        "sourceMaps": true
    }, {
        "name": "Attach",
        "type": "node",
        "request": "attach",
        "port": 5858,
        "address": "localhost",
        "restart": false,
        "sourceMaps": true,
        "localRoot": "${workspaceRoot}",
        "remoteRoot": null
    }]
}